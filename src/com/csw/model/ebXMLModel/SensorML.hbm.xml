<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.csw.model.ebXMLModel.SensorML" table="sensorml"
		dynamic-insert="true" dynamic-update="true">
		<id name="id" column="id">
			<generator class="increment"></generator>
		</id>
		<property name="sensorid" column="sensorid"></property>
		<property name="sensorcontent" column="sensorcontent" type="java.sql.Clob"
			not-null="true"></property>
		<property name="username" column="username"></property>
		<property name="savetime" column="savetime" type="timestamp"></property>
	</class>
</hibernate-mapping>
