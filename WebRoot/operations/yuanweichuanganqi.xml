<SensorML version="1.0.1"
	xsi:schemaLocation="http://www.opengis.net/sensorML/1.0.1 http://schemas.opengis.net/sensorML/1.0.1/sensorML.xsd"
	xmlns="http://www.opengis.net/sensorML/1.0.1" xmlns:gml="http://www.opengis.net/gml"
	xmlns:swe="http://www.opengis.net/swe/1.0.1" xmlns:xlink="http://www.w3.org/1999/xlink"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<member>
		<!--===============-->
		<!-- 地面原位传感器 -->
		<!--===============-->
		<System gml:id="InsituSensor">
			<gml:description>原位传感器概述</gml:description>
			<gml:name>地面原位传感器</gml:name>
			<!--======================-->
			<!--  传感器标签==>关键词  -->
			<!--======================-->
			<!-- YXL 已经转换 -->
			<keywords>
				<KeywordList>
					<keyword>原位</keyword>
					<keyword>水文/气象/水质/大气/...</keyword>
					<keyword>传感器</keyword>
					<keyword>典型应用</keyword>
				</KeywordList>
			</keywords>
			<!--======================-->
			<!--  传感器标签==>标示符  -->
			<!--======================-->
			<!-- YXL 已经转换 -->
			<identification>
				<IdentifierList>
					<identifier name="标识码">
						<Term definition="urn:ogc:def:identifier:OGC:1.0:uniqueID">
							<value>urn:ogc:feature:insitusensor:传感器简称大写(不能有空格等)</value>
						</Term>
					</identifier>
					<identifier name="全称">
						<Term definition="urn:ogc:def:identifier:OGC:1.0:longName">
							<value>null</value>
						</Term>
					</identifier>
					<identifier name="简称">
						<Term definition="urn:ogc:def:identifier:OGC:1.0:shortName">
							<value>null</value>
						</Term>
					</identifier>
					<identifier name="所属平台名称">
						<Term definition="urn:ogc:def:identifier:OGC:1.0:parentSystemStandardName">
							<value>平台简称</value>
						</Term>
					</identifier>
					<identifier name="【*】所属平台标识码">
						<Term definition="urn:ogc:def:identifier:OGC:1.0:parentSystemUniqueId">
							<value>urn:ogc:feature:insitusensor:platform:XX</value>
						</Term>
					</identifier>
				</IdentifierList>
			</identification>
			<!--======================-->
			<!--  传感器标签==>标示符  -->
			<!--======================-->
			<!-- YXL 已经转换 -->
			<classification>
				<ClassifierList>
					<classifier name="【*】传感器类型">
						<Term definition="urn:ogc:def:classifier:OGC:1.0:sensorType">
							<value>地面原位传感器</value>
						</Term>
					</classifier>
					<classifier name="【*】传感器类型">
						<Term definition="urn:ogc:def:classifier:OGC:1.0:sensorType">
							<value>水位计/温度计/....</value>
						</Term>
					</classifier>
					<classifier name="【*】预期应用">
						<Term definition="urn:ogc:def:classifier:OGC:1.0:intendedApplication">
							<value>水文观测/水质观测/气象观测/大气观测/...</value>
						</Term>
					</classifier>
					<classifier name="【*】预期应用">
						<Term definition="urn:ogc:def:classifier:OGC:1.0:newAdded">
							<value>flood【必填】</value>
						</Term>
					</classifier>
					<classifier name="预期应用">
						<Term definition="urn:ogc:def:classifier:OGC:1.0:newAdded">
							<value>应用1（此结构可重复）</value>
						</Term>
					</classifier>
				</ClassifierList>
			</classification>
			<!--=============================-->
			<!--  传感器互操作服务==>有效时间  -->
			<!--=============================-->
			<!-- YXL 已经转换 -->
			<validTime>
				<gml:TimePeriod gml:id="传感器有效时间">
					<gml:beginPosition>2011-01-01T00:00:00.0Z</gml:beginPosition>
					<gml:endPosition>2011-01-01T00:00:00.0Z</gml:endPosition>
				</gml:TimePeriod>
			</validTime>
			<!--===========================-->
			<!--  传感器物理特征==>物理特征  -->
			<!--===========================-->
			<!-- YXL 已经转换 -->
			<characteristics>
				<swe:DataRecord definition="urn:ogc:def:property:Liesmars:physicalProperties">
					<swe:field name="长度">
						<swe:Quantity definition="urn:ogc:def:property:length">
							<swe:uom code="m" />
							<swe:value>00</swe:value>
						</swe:Quantity>
					</swe:field>
					<swe:field name="宽度">
						<swe:Quantity definition="urn:ogc:def:property:width">
							<swe:uom code="m" />
							<swe:value>00</swe:value>
						</swe:Quantity>
					</swe:field>
					<swe:field name="高度">
						<swe:Quantity definition="urn:ogc:def:property:height">
							<swe:uom code="m" />
							<swe:value>00</swe:value>
						</swe:Quantity>
					</swe:field>
					<swe:field name="质量">
						<swe:Quantity definition="urn:ogc:def:property:weight">
							<swe:uom code="kg" />
							<swe:value>00</swe:value>
						</swe:Quantity>
					</swe:field>
					<swe:field name="电源类型">
						<swe:Category definition="urn:ogc:def:property:powerType">
							<swe:value>直流电源/交流电源</swe:value>
						</swe:Category>
					</swe:field>
					<swe:field name="电流">
						<swe:Quantity definition="urn:ogc:def:property:current">
							<swe:uom code="A" />
							<swe:value>00</swe:value>
						</swe:Quantity>
					</swe:field>
					<swe:field name="电压">
						<swe:Quantity definition="urn:ogc:def:property:voltage">
							<swe:uom code="V" />
							<swe:value>00</swe:value>
						</swe:Quantity>
					</swe:field>
					<swe:field name="功率">
						<swe:Quantity definition="urn:ogc:def:property:power">
							<swe:uom code="W" />
							<swe:value>00</swe:value>
						</swe:Quantity>
					</swe:field>
					<swe:field name="输出功率">
						<swe:Quantity definition="urn:ogc:def:property:outputPower">
							<swe:uom code="W" />
							<swe:value>00</swe:value>
						</swe:Quantity>
					</swe:field>
					<swe:field name="设计寿命">
						<swe:Quantity definition="urn:ogc:def:property:designedLife">
							<swe:uom code="year" />
							<swe:value>00</swe:value>
						</swe:Quantity>
					</swe:field>
					<swe:field name="工作温度范围">
						<swe:QuantityRange definition="urn:ogc:def:property:operatingTemperature">
							<swe:uom code="cel" />
							<swe:value>00 00</swe:value>
						</swe:QuantityRange>
					</swe:field>
					<swe:field name="工作湿度范围">
						<swe:QuantityRange definition="urn:ogc:def:property:operatingHumidity">
							<swe:uom code="%" />
							<swe:value>00 00</swe:value>
						</swe:QuantityRange>
					</swe:field>
					<swe:field name="防护等级">
						<swe:Text definition="urn:ogc:def:property:protectionGrade">
							<swe:value>null</swe:value>
						</swe:Text>
					</swe:field>
					<swe:field name="无故障时间">
						<swe:Quantity definition="urn:ogc:def:property:MTBF">
							<swe:uom code="h" />
							<swe:value>00</swe:value>
						</swe:Quantity>
					</swe:field>
				</swe:DataRecord>
			</characteristics>
			<!--===========================-->
			<!--  传感器观测能力==>观测广度  -->
			<!--  传感器观测能力==>观测深度  -->
			<!--  传感器观测能力==>观测频度  -->
			<!--  传感器观测能力==>观测质量  -->
			<!--  传感器观测能力==>观测数据  -->
			<!--===========================-->

			<capabilities>
				<swe:DataRecord
					definition="urn:ogc:def:property:Liesmars:measurementCapabilities">
					<!-- YXL 已经转换 -->
					<!--  传感器观测能力==>观测广度  -->
					<swe:field name="observedBBOX">
						<swe:Envelope definition="urn:ogc:def:property:OGC:1.0:observedBBOX"
							referenceFrame="urn:ogc:def:crs:EPSG:4326">
							<swe:lowerCorner>
								<swe:Vector>
									<swe:coordinate name="纬度">
										<swe:Quantity axisID="y">
											<swe:uom code="deg" xlink:href="urn:ogc:def:uom:UCUM::deg" />
											<swe:value>-90</swe:value>
										</swe:Quantity>
									</swe:coordinate>
									<swe:coordinate name="经度">
										<swe:Quantity axisID="x">
											<swe:uom code="deg" xlink:href="urn:ogc:def:uom:UCUM::deg" />
											<swe:value>-180</swe:value>
										</swe:Quantity>
									</swe:coordinate>
								</swe:Vector>
							</swe:lowerCorner>
							<swe:upperCorner>
								<swe:Vector>
									<swe:coordinate name="纬度">
										<swe:Quantity axisID="y">
											<swe:uom code="deg" xlink:href="urn:ogc:def:uom:UCUM::deg" />
											<swe:value>90</swe:value>
										</swe:Quantity>
									</swe:coordinate>
									<swe:coordinate name="经度">
										<swe:Quantity axisID="x">
											<swe:uom code="deg" xlink:href="urn:ogc:def:uom:UCUM::deg" />
											<swe:value>180</swe:value>
										</swe:Quantity>
									</swe:coordinate>
								</swe:Vector>
							</swe:upperCorner>
						</swe:Envelope>
					</swe:field>
					<!-- YXL 已经转换 -->
					<swe:field name="观测原理">
						<swe:Text definition="urn:ogc:def:property:observationPrinciple">
							<swe:value>null(气泡式/超声波/...)</swe:value>
						</swe:Text>
					</swe:field>
					<!-- YXL 已经转换 -->
					<swe:field name="观测范围">
						<swe:QuantityRange definition="urn:ogc:def:property:observationRange">
							<swe:uom code="null" />
							<swe:value>00 00</swe:value>
						</swe:QuantityRange>
					</swe:field>
					<!-- YXL 已经转换 -->
					<swe:field name="观测半径">
						<swe:Quantity definition="urn:ogc:def:property:observationRadius">
							<swe:uom code="m" />
							<swe:value>00</swe:value>
						</swe:Quantity>
					</swe:field>
					<!--  传感器观测能力==>观测深度  -->
					<!-- YXL 已经转换 -->
					<swe:field name="观测分辨率">
						<swe:Quantity definition="urn:ogc:def:property:observationResolution">
							<swe:uom code="null" />
							<swe:value>00</swe:value>
						</swe:Quantity>
					</swe:field>
					<!-- YXL 已经转换 -->
					<swe:field name="探测精度">
						<swe:QuantityRange definition="urn:ogc:def:property:precisionRange">
							<swe:uom code="%" />
							<swe:value>5 5</swe:value>
						</swe:QuantityRange>
					</swe:field>
					<!--  传感器观测能力==>观测频度  -->
					<!-- YXL 已经转换 -->
					<swe:field name="观测频率">
						<swe:Quantity definition="urn:ogc:def:property:observingFrequency">
							<swe:uom code="day/hour/min/s" />
							<swe:value>00</swe:value>
						</swe:Quantity>
					</swe:field>
					<!-- YXL 已经转换 -->
					<swe:field name="时间分辨率范围">
						<swe:QuantityRange definition="urn:ogc:def:property:temporalResolutionRange">
							<swe:uom code="d" />
							<swe:value>00 00</swe:value>
						</swe:QuantityRange>
					</swe:field>
					<!--  传感器观测能力==>观测质量  -->
					<!-- YXL 已经转换 -->
					<swe:field name="观测绝对误差">
						<swe:Quantity definition="urn:ogc:def:property:observationAbsError">
							<swe:uom code="null" />
							<swe:value>00</swe:value>
						</swe:Quantity>
					</swe:field>
					<!-- YXL 已经转换 -->
					<swe:field name="观测相对误差">
						<swe:Quantity definition="urn:ogc:def:property:observationRelativeError">
							<swe:uom code="%" />
							<swe:value>00</swe:value>
						</swe:Quantity>
					</swe:field>

					<!--  传感器观测能力==>观测数据  -->
					<!-- YXL 已经转换 -->
					<swe:field name="数据格式">
						<swe:Category definition="urn:ogc:def:property:dataFormat">
							<swe:value>null</swe:value>
						</swe:Category>
					</swe:field>
					<!-- YXL 已经转换 -->
					<swe:field name="数据质量等级">
						<swe:Category definition="urn:ogc:def:property:dataQualityLevel">
							<swe:value>null</swe:value>
						</swe:Category>
					</swe:field>
					<!-- YXL 已经转换 -->
					<swe:field name="数据获取等级">
						<swe:Category definition="urn:ogc:def:property:dataAccessLevel">
							<swe:value>null</swe:value>
						</swe:Category>
					</swe:field>
				</swe:DataRecord>
			</capabilities>
			<!--=============================-->
			<!--  传感器互操作服务==>联系单位  -->
			<!--=============================-->
			<contact xlink:arcrole="urn:ogc:def:role:operator">
				<ResponsibleParty>
					<individualName>null</individualName>
					<organizationName>null（中文名称加括号英文简称）</organizationName>
					<contactInfo>
						<phone>
							<voice>null</voice>
							<facsimile>null</facsimile>
						</phone>
						<address>
							<deliveryPoint>null</deliveryPoint>
							<city>null</city>
							<administrativeArea>null</administrativeArea>
							<postalCode>null</postalCode>
							<country>null</country>
							<electronicMailAddress>null</electronicMailAddress>
						</address>
					</contactInfo>
				</ResponsibleParty>
			</contact>
			<!--=============================-->
			<!--  传感器互操作服务==>服务接口  -->
			<!--=============================-->
			<interfaces>
				<InterfaceList>
					<interface name="Temperature001_SOS(示例)">
						<InterfaceDefinition>
							<serviceLayer>
								<swe:DataRecord
									definition="urn:ogc:def:interface:OGC:1.0:SWEServiceInterface">
									<swe:field name="urn:ogc:def:interface:OGC:1.0:ServiceURL">
										<swe:Text>
											<swe:value>http://swe.whu.edu.cn:9000/SOS/Temperature001_SOS
											</swe:value>
										</swe:Text>
									</swe:field>
									<swe:field name="urn:ogc:def:interface:OGC:1.0:ServiceType">
										<swe:Text>
											<swe:value>SOS</swe:value>
										</swe:Text>
									</swe:field>
									<swe:field
										name="urn:ogc:def:interface:OGC:1.0:ServiceSpecificSensorID">
										<swe:Text>
											<swe:value>urn:ogc:feature:insitusensor:Temperature001
											</swe:value>
										</swe:Text>
									</swe:field>
								</swe:DataRecord>
							</serviceLayer>
						</InterfaceDefinition>
					</interface>
				</InterfaceList>
			</interfaces>
			<!--============-->
			<!--  输入输出  -->
			<!--============-->
			<!-- YXL 已经转换 -->
			<inputs>
				<InputList>
					<input name="大气温度(示例)">
						<swe:ObservableProperty
							definition="urn:ogc:def:property:OGC:1.0:AirTemperature" />
					</input>
				</InputList>
			</inputs>
			<!-- YXL 已经转换 -->
			<outputs>
				<OutputList>
					<output name="温度(示例)">
						<swe:Quantity definition="urn:ogc:def:property:OGC:1.0:Temperature">
							<swe:uom code="cel" />
						</swe:Quantity>
					</output>
				</OutputList>
			</outputs>
		</System>
	</member>
</SensorML>