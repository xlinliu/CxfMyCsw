<?xml version="1.0" encoding="UTF-8"?>
<GetRecords xmlns="http://www.opengis.net/cat/csw/2.0.2"
	xmlns:csw="http://www.opengis.net/cat/csw/2.0.2" xmlns:ogc="http://www.opengis.net/ogc"
	xmlns:ows="http://www.opengis.net/ows" xmlns:dc="http://purl.org/dc/elements/1.1/"
	xmlns:dct="http://purl.org/dc/terms/" xmlns:gml="http://www.opengis.net/gml"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" service="CSW"
	version="2.0.2" maxRecords="1" startPosition="1" resultType="results"
	outputFormat="application/xml" outputSchema="http://www.opengis.net/cat/csw/2.0.2"
	xsi:schemaLocation="http://www.opengis.net/cat/csw/2.0.2 
	 ../../../csw/2.0.2/CSW-discovery.xsd">
	<Query typeNames="csw:Record">
		<ElementSetName typeNames="csw:Record">full</ElementSetName>
		<Constraint version="1.1.0">
			<ogc:Filter>
				<ogc:And>
	<!--
		<ogc:PropertyIsLike escapeChar="\" singleChar="?" wildCard="*">
		<ogc:PropertyName>dc:title</ogc:PropertyName>
		<ogc:Literal>System</ogc:Literal> </ogc:PropertyIsLike>
		<ogc:PropertyIsEqualTo> <ogc:PropertyName>dc:type</ogc:PropertyName>
		<ogc:Literal>System</ogc:Literal> </ogc:PropertyIsEqualTo>
		<ogc:PropertyIsGreaterThanOrEqualTo>
		<ogc:PropertyName>dct:start</ogc:PropertyName> <ogc:Literal>1990-01-01
		00:00:00</ogc:Literal> </ogc:PropertyIsGreaterThanOrEqualTo>
		<ogc:PropertyIsLessThanOrEqualTo>
		<ogc:PropertyName>dct:end</ogc:PropertyName> <ogc:Literal>2012-01-01
		23:59:59</ogc:Literal> </ogc:PropertyIsLessThanOrEqualTo>
		<ogc:Intersects> <ogc:PropertyName>ows:BoundingBox</ogc:PropertyName>
		<gml:Envelope> <gml:lowerCorner>30 10</gml:lowerCorner>
		<gml:upperCorner>-30 -10</gml:upperCorner> </gml:Envelope>
		</ogc:Intersects>
	--></ogc:And>
			</ogc:Filter>
		</Constraint>
	</Query>
</GetRecords>
