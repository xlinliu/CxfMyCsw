<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.csw.model.ebXMLModel.LocalizedString" table="localizedString"
		dynamic-insert="true" dynamic-update="true">
		<id name="outid" column="outid">
			<generator class="increment"></generator>
		</id>
		<property name="id" column="id"></property>
		<property name="lang" column="lang"></property>
		<property name="value" column="value"></property>
		<property name="charset" column="charset"></property>
	</class>
</hibernate-mapping>
