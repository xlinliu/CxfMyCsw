<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.event.JavaBeans.EventDenagedRoadJBean" table="event_denagedroad_info">
		<id name="demagedRoadId" column="demangedroadid">
			<generator class="increment"></generator>
		</id>
		<property name="breakPointLocation" column="breakpointlocation"
			not-null="true"></property>
		<property name="conditionDescription" column="conditioindescriptioin"
			not-null="true"></property>
		<property name="damagedRoadInfo" column="damagedroadinfo"
			not-null="true"></property>
		<property name="eventid" column="event_id" not-null="true"></property>
	</class>
</hibernate-mapping>
